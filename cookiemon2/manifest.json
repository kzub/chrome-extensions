{
   "author": "Shahul Hameed, N",
   "background": {
      "persistent": true,
      "scripts": [ "cookiemon-background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "images/icon19.png",
         "38": "images/icon38.png"
      },
      "default_title": "Cookie Monitor"
   },
   "description": "This extension monitors the cookies addition and deletion",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "48": "images/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Cookie Monitor",
   "permissions": [ "tabs", "cookies", "storage", "webRequest", "https://*/*", "http://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0"
}
